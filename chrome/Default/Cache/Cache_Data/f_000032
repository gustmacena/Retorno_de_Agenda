 <script>

    $(document).on("keypress", "#modal-agendamento-checkin [data-mask='cpf']", function(e) {
        input = $(this);
        modal = $("#modal-agendamento-checkin");
        old_value = $(this).attr('old_value');

        setTimeout(function() {
            if(input.attr('old_value') != input.val()) {
                switch(input.attr('name')) {
                    case 'cpf_motorista':
                        target_nome = modal.find('[name=nome_motorista]');
                        target_celular = modal.find('[name=celular_motorista]');
                        target_rngc = modal.find('[name=rngc_motorista]');
                    break;

                    default:
                        num_ajudante = input.attr('name').split('cpf_ajudante');
                        num_ajudante = num_ajudante[1];
                        target_nome = modal.find('[name=nome_ajudante'+num_ajudante+']');
                        target_celular = modal.find('[name=celular_ajudante'+num_ajudante+']');
                        target_rngc = modal.find('[name=rngc_ajudante'+num_ajudante+']');
                    break;
                }

                if(input.data('value').length == 11) {
                    modalVeicMotConsultarMotorista(input.val(), target_nome, target_celular, target_rngc);
                } else {
                    target_nome.val('').attr('readonly', true);
                    target_celular.val('').attr('readonly', true);
                    target_rngc.val('').attr('readonly', true);
                }
            }
            input.attr('old_value', input.val());
        }, 0);
    });

    function salvarVeiculoMotorista(){
        modal = $('#modal-agendamento-checkin');
        form = modal.find('form');
        limparErrosFormulario(form);

        $.ajax({
            url: "https://atacadao.hodiebooking.com.br/agendamentos/2407050048370/dados-veiculo",
            method: "PUT",
            data: modal.find('form').serialize(),
            success: function(json) {
                 guardar();
                 salvarMotorista();
            },
            statusCode: {

                422: function(json) {
                    var resp = json.responseJSON;
                    destacarErrosFormulario(resp.errors, modal);
                }
            }
        });
    }

    function salvarMotorista(){
        $('.msg_erros_notas').hide();
        modal = $('#modal-agendamento-checkin');
        form = modal.find('form');
        limparErrosFormulario(form);

        $('.msg_erros_notas').hide();

        $.ajax({
            url: "https://atacadao.hodiebooking.com.br/agendamentos/2407050048370/checkin",
            method: "PUT",
            data: modal.find('form').serialize(),
            success: function(json) {
                $(document).on('hide.bs.modal', '#modal-alert', function() {
                    window.location.href = json.protocolo;
                });
                alerta('Registro atualizado com sucesso!');
            },
            statusCode:{
                422: function(json){
                    objErro = {};

                    var resp = json.responseJSON;
                    destacarErrosFormulario(resp.errors, modal);

                    if(form.find('.form-group.has-error').length > 0) {
                        objErro = {
                            obj: $('#modal-agendamento-checkin .form-group.has-error').get(0),
                            margin: 35
                        };
                    }

                    if(typeof resp.errors.nota != 'undefined') {
                        $(".msg_erros_notas").html(resp.errors.nota[0]).show();
                        $(resp.errors_notas).each(function(k,v){
                            $("[name^='nota"+v+"']").parents('.form-group').first().addClass('has-error').addClass('error');
                        });
                        if(jQuery.isEmptyObject(objErro)){
                            objErro = {
                                obj: $('#modal-agendamento-checkin .msg_erros_notas').get(0),
                                margin: 5
                            };
                        }
                    }

                    if(!jQuery.isEmptyObject(objErro)) {
                        moverModalPara(objErro.obj, objErro.margin, modal);
                    }

                    //Se o erro da fila for o único erro
                    if(Object.keys(resp.errors).length == 1 && typeof resp.errors.fila != 'undefined') {
                        confirmar(resp.errors.fila[0], function() {
                            $('#modal-agendamento-checkin [name=validar_fila]').val('false');
                            $('#modal-agendamento-checkin .salvar').click();
                        });
                    }
                }
            }
        });
    }


    /* **************************** */
    $(document).on('click', '#modal-agendamento-checkin .salvar', function() {
        salvarVeiculoMotorista();
    });


    function guardar() {

        let modal = $("#modal-agendamento-checkin");
        let inp_cpf = modal.find("[name=cpf_motorista]");
        if(inp_cpf.val() != '' && inp_cpf.val().replace(/\D/g, '').length == 11) {
            modal.find("[name=celular_motorista]").attr('readonly', false);
            modal.find("[name=rngc_motorista]").attr('readonly', false);
        }

        modal.find("[name^='cpf_ajudante']").each(function() {
            if($(this).val() != '' && $(this).val().replace(/\D/g, '').length == 11) {
                num_ajudante = $(this).attr('name').split('cpf_ajudante');
                num_ajudante = num_ajudante[1];
                modal.find("[name=celular_ajudante"+num_ajudante+"]").attr('readonly', false);
                //modal.find("[name=rngc_ajudante"+num_ajudante+"]").attr('readonly', false);
            }
        })

    }

    // Campos de notas fiscais
    function actualizar() {
        let modal = $("#modal-agendamento-checkin");
        let inp_cpf = modal.find("[name=cpf_motorista]");
        if(inp_cpf.val() != '' && inp_cpf.val().replace(/\D/g, '').length == 11) {
            modal.find("[name=celular_motorista]").attr('readonly', false);
        }

        modal.find("[name^='cpf_ajudante']").each(function(){
            if($(this).val() != '' && $(this).val().replace(/\D/g, '').length == 11) {
                num_ajudante = $(this).attr('name').split('cpf_ajudante');
                num_ajudante = num_ajudante[1];
                modal.find("[name=celular_ajudante"+num_ajudante+"]").attr('readonly', false);
            }
        });
    };



    $(document).on('keyup focus', '#modal-agendamento-checkin [name^="nota["]', function() {
        if($(this).is('[name^="nota["]:last') && $(this).attr('readonly') != 'readonly' /*&& $(this).val() != ''*/) {
            prox_num = parseInt($(this).parents('.form-group').first().find('.input-group-addon').text()) + 1;
            if (prox_num > 300) return false;
            html = '' +
                '<div class="row notas-campos">' +
                '<div class="col-md-6 form-group nota">' +
                    '<div class="input-group">' +
                    '<span class="input-group-addon" id="basic-addon'+prox_num+'">'+prox_num+'</span>' +
                    '<input type="text"  maxlength="44" class="form-control inteiro"  name="nota['+prox_num+']" aria-describedby="basic-addon'+prox_num+'">' +
                    '</div>' +
                '</div>' +
                '<div class="col-md-6">' +
                    '<i style="margin-top: 10px" class="fa fa-plus orange"></i>' +
                '</div>' +
                '</div>';
            $("#modal-agendamento-checkin #notas").append(html);
            mascaraInteiro(null,true);
        }
    });

    $(document).on('click', '#modal-agendamento-checkin .btn-alterar-tipo-carga, #modal-agendamento-checkin .btn-alterar-tipo-veiculo', function() {
        $("#modal-agendamento-checkin").modal('hide');
    });

    $('#modal-agendamento-checkin').on('show.bs.modal', function (e) {
        if (e.namespace == 'bs.modal') {
            $("#modal-agendamento-checkin [name=validar_fila]").val("true");
        }
    });
</script>

