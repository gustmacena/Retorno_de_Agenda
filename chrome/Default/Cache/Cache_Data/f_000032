  <script>
  $(document).on('click', '#modal-agendamento-no-show .salvar', function(){
    modal = $('#modal-agendamento-no-show');
    form = modal.find('form');
    limparErrosFormulario(form);

    $.ajax({
      url: "https://atacadao.hodiebooking.com.br/agendamentos/2407050048370/noshow",
      method: "PUT",
      data: modal.find('form').serialize(),
      success: function(json){
        $(document).on('hide.bs.modal', '#modal-alert', function(){
          window.location.href = json.protocolo;
        });
        alerta('Registro atualizado com sucesso!');
      },
      statusCode:{
        422: function(json){
          objErro = {};

          var resp = json.responseJSON;
          destacarErrosFormulario(resp.errors, modal);

          if(form.find('.form-group.has-error').length > 0){
            objErro = {
              obj: $('#modal-agendamento-no-show .form-group.has-error').get(0),
              margin: 35
            };
          }

          if(!jQuery.isEmptyObject(objErro)){
            moverModalPara(objErro.obj, objErro.margin, modal);
          }
        }
      }
    });
  });

  $(document).on('click', '#modal-agendamento-no-show [name=estou_ciente]', function(){
    if($(this).prop('checked')){
      $("#modal-agendamento-no-show .salvar").prop('disabled', false);
    }else{
      $("#modal-agendamento-no-show .salvar").prop('disabled', true);
    }
  });
