  <script>

    var vue_modal_descarregamento = new Vue({
      el: '#vue_modal_descarregamento',
      data: {
        agendamento: {},
      },
      mounted: function(){
        //applyMaskCelular();
      },
      methods: {
        descarregar(){

          let modal = $('#modal-agendamento-descarregamento');
          limparErrosFormulario(modal);

          form = modal.find('[name=formDescarregar]');

          var data = form.serialize();
          var url = "https://atacadao.hodiebooking.com.br/agendamentos/:protocolo/descarregar";
          url = url.replace(':protocolo', this.agendamento.protocolo);

          $.ajax({
            data: data,
            url: url,
            method: "PUT",
            success: function(json){
              alerta('Registro atualizado com sucesso');
              modal.modal('hide');
              if(typeof modal.data('function_after_save') != 'undefined'){
                modal.data().function_after_save(json);
              }
            },
        		statusCode:{
        			422: function(json){
        				var resp = json.responseJSON;
                destacarErrosFormulario(resp.errors, form);
              }
            },
          });
        },
      },
      watch: {
        agendamento: function(val, oldval){
          if(val.id_agendamento != oldval.id_agendamento && typeof oldval.id_agendamento != 'undefined'){
            let modal = $('#modal-agendamento-descarregamento');
            limparErrosFormulario(modal);
            modal.find('[data-target_datetimepicker="dthr_descarregamento"][data-action="remove"]').click();
          }
        }
      }
    });

    function formatDataHora(data){
      return moment(data).format('DD/MM/YYYY HH:mm');
    }

    function modalDescarregamentoSetAgendamento(agendamento){
      var date = new Date();
      if (agendamento.timezoneUni !== undefined) {
        var diff =  agendamento.timezoneUni * 60 + date.getTimezoneOffset();
      } else {
        var diff =  -3 * 60 + date.getTimezoneOffset();
      }
      var data = new Date(date.getTime() + diff * 60 * 1000 ); 
      agendamento.newData = formatDataHora(data);
      if ((agendamento.hasOwnProperty('transportadora')) && (agendamento.transportadora.hasOwnProperty('nome'))) {
        agendamento.transportadora = agendamento.transportadora.nome;
      }
      Vue.set(vue_modal_descarregamento, 'agendamento', agendamento);
    }
