  <script>
  
    $(document).on('blur', '#modal-agendamento-desfazer-descarregamento [name=posicao]', function(){
      if($(this).val() != ''){
        if(typeof $(this).attr('max') != 'undefined' && $(this).attr('max') != '' && parseInt($(this).val()) > parseInt($(this).attr('max'))){
          $(this).val($(this).attr('max'));
        }else if(typeof $(this).attr('min') != 'undefined' && $(this).attr('min') != '' && parseInt($(this).val()) < parseInt($(this).attr('min'))){
          $(this).val($(this).attr('min'));
        }
      }
    });
    
    $(document).on('click', '#modal-agendamento-desfazer-descarregamento .btn_add_posicao, #modal-agendamento-desfazer-descarregamento .btn_sub_posicao', function(){
      input = $("#modal-agendamento-desfazer-descarregamento [name=posicao]");
      
      if(input.val() == ''){
        input.val(0);
      }
      
      aux = -1;
      if($(this).hasClass('btn_add_posicao')){
        aux = 1;
      }
      
      input.val(parseInt(input.val()) + aux);
      
      if(input != ''){
        if(typeof input.attr('max') != 'undefined' && input.attr('max') != '' && parseInt(input.val()) > parseInt(input.attr('max'))){
          input.val(input.attr('max'));
        }else if(typeof input.attr('min') != 'undefined' && input.attr('min') != '' && parseInt(input.val()) < parseInt(input.attr('min'))){
          input.val(input.attr('min'));
        }
      }
    });
  
    var vue_modal_desfazer_descarregamento = new Vue({
      el: '#vue_modal_desfazer_descarregamento',
      data: {
        agendamento: {},
      },
      mounted: function(){
        
      },
      methods: {
        desfazerDescarregamento(){
          
          let modal = $('#modal-agendamento-desfazer-descarregamento');
          limparErrosFormulario(modal);

          form = modal.find('[name=formDesfazerDescarregamento]');
          
          var data = form.serialize();
          var url = "https://atacadao.hodiebooking.com.br/agendamentos/:protocolo/desfazer-descarregamento";
          url = url.replace(':protocolo', this.agendamento.protocolo);
          
          $.ajax({
            data: data,
            url: url,
            method: "PUT",
            success: function(json){
              alerta('Registro atualizado com sucesso');
              modal.modal('hide');
              
              if(typeof modal.data('function_after_save') != 'undefined'){
                modal.data().function_after_save(json);
              }
            },
        		statusCode:{
        			422: function(json){
        				var resp = json.responseJSON;
                destacarErrosFormulario(resp.errors, form);                
              }
            },
          });
        },
        getAvisoFila(){
          fila = "";
          fila_agendamento = this.agendamento.fila_agendamento;
          if(fila_agendamento !== undefined){
            if(fila_agendamento.fila !== undefined){
              if(fila_agendamento.flag_prioridade){
                fila = 'Priorizados';
              }else{
                fila = fila_agendamento.fila.nome;
              }
              fila = ': "' + fila + '"';
            }
          }
          
          msg = 'Ao desfazer o descarregamento, o veículo voltará para a fila:fila. É necessário informar a posição na fila para prosseguir.';
          msg = msg.replace(':fila', fila);
          return msg;
        }
      },
      watch: {
        agendamento: function(val, oldval){
          let modal = $('#modal-agendamento-desfazer-descarregamento');
          if(val.id_agendamento != oldval.id_agendamento && typeof oldval.id_agendamento != 'undefined'){
            limparErrosFormulario(modal);
          }
          if(typeof val.id_agendamento != 'undefined'){
            max = "";
            if(typeof val.qtd_agendamentos_fila != undefined){
              max = parseInt(val.qtd_agendamentos_fila) + 1;
            }
            modal.find('[name=posicao]').val(max).attr('max', max);
          }
        }
      }
    });
    
    function modalDesfazerDescarregamentoSetAgendamento(agendamento){
      Vue.set(vue_modal_desfazer_descarregamento, 'agendamento', agendamento);
    }
    
