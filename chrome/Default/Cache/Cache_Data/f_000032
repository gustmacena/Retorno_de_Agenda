  <script>

    var vue_modal_chamada_motorista = new Vue({
      el: '#vue_modal_chamada_motorista',
      data: {
        agendamento: {},
        qtdCaracteresMensagem: 0,
        tabAtiva: 'chamar_por_sms',
        columns_historico: ['indice', 'data', 'fila', 'chamado_por', 'celular'],
        options_historico: {
          headings: {
            indice: '',
            data: 'Data',
            fila: 'Fila',
            chamado_por: 'Chamado por',
            celular: 'Celular',
          },
          sortable: [],
          orderBy: {
            column: 'data',
            ascending:false
          },
          perPage: 10,
          skin: 'table table-hover table-condensed',
        },
      },
      mounted: function(){
    //    applyMaskCelular();
      },
      methods: {
        getMensagemPadrao(){
          nome_motorista = this.getValueByDotString(this.agendamento, 'motorista.nome');
          if(nome_motorista == null){
            nome_motorista = 'motorista';
          }

          msg = 'Olá :nome_motorista. Por favor se apresente na doca para iniciar o descarregamento.';
          msg = msg.replace(':nome_motorista', nome_motorista);
          return msg;
        },
        chamarMotorista(ignorar_ocorrencias){

          let modal = $('#modal-agendamento-chamada-motorista');
          limparErrosFormulario(modal);

          prosseguir = true;

          if(typeof ignorar_ocorrencias == 'undefined'){
            ignorar_ocorrencias = false;
          }

          if(!ignorar_ocorrencias){
            if(typeof this.agendamento.ocorrencias != 'undefined' && this.agendamento.ocorrencias.length > 0){
              possui_ocorrencia = false;
              $(this.agendamento.ocorrencias).each(function(koco, oco){
                if(oco.flag_finalizada == false){
                  possui_ocorrencia = true;
                  prosseguir = false;
                }
              });

              if(possui_ocorrencia) {
                confirmar('Este agendamento possui ocorrências abertas. Deseja realmente chamar o motorista?', function(){
                  vue_modal_chamada_motorista.chamarMotorista(true);
                })
              }
            }
          }

          if(!prosseguir){
            return false;
          }

          if(this.tabAtiva == 'chamar_por_sms'){
            form = modal.find('#chamar_por_sms form');
          } else if(this.tabAtiva == 'chamar_por_outro'){
            form = modal.find('#chamar_por_outro form');
          } else {
            return false;
          }

          var data = form.serialize();
          var url = "https://atacadao.hodiebooking.com.br/agendamentos/:protocolo/chamar-motorista";
          url = url.replace(':protocolo', this.agendamento.protocolo);

          $.ajax({
            data: data,
            url: url,
            method: "PUT",
            success: function(json){
              if(vue_modal_chamada_motorista.tabAtiva == 'chamar_por_sms') {
                msg = 'SMS enviado com sucesso';
              }else{
                msg = 'Registro da chamada salvo com sucesso';
              }
              alerta(msg);
              modal.modal('hide');
              location.reload();
              vue_modal_chamada_motorista.agendamento.chamadas_motoristas.push(json);
              if(typeof modal.data('function_after_save') != 'undefined'){
                modal.data().function_after_save(json);   
                          
              }
              
            },
        		statusCode:{
        			422: function(json){
        				var resp = json.responseJSON;
                destacarErrosFormulario(resp.errors, form);

                if(typeof resp.errors.sms != 'undefined'){
                  alerta(resp.errors.sms[0], true);
                }

                else if(typeof resp.errors.chamada_motorista != 'undefined'){
                  alerta(resp.errors.chamada_motorista[0], true);
                }

              }
            },
          });
        },
        atualizaQtdCaracteresMensagem(){
          this.qtdCaracteresMensagem = $('#modal-agendamento-chamada-motorista #chamar_por_sms [name=mensagem]').val().length;
        },
        setTabAtiva(tab){
          this.tabAtiva = tab;
        },
        countChamadasMotoristas(){
          return typeof this.agendamento.chamadas_motoristas != 'undefined' ? this.agendamento.chamadas_motoristas.length : 0;
        },
        getIndiceChamadaMotorista(index){
          return this.countChamadasMotoristas() - parseInt(index - 1);
        },
        getNomeFila(chamada){
          if(chamada.flag_prioridade == true){
            nome_fila = 'Priorizados';
          }else{
            nome_fila = chamada.fila.nome;
          }
          return nome_fila;
        }
      },
      watch: {
        agendamento: function(val, oldval){
          if(val.id_agendamento != oldval.id_agendamento){
            let modal = $('#modal-agendamento-chamada-motorista');
            modal.find('#chamar_por_sms [name=mensagem]').val(this.getMensagemPadrao());
            modal.find('[name=celular]').val('');
            modal.find('#chamar_por_outro [name=mensagem]').val('');
            this.atualizaQtdCaracteresMensagem();
          }
        }
      }
    });

        function modalChamadaMotoristaSetAgendamento(agendamento){
      Vue.set(vue_modal_chamada_motorista, 'agendamento', agendamento);
    }
