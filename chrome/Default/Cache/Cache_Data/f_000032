        /** CANCELAR CARGA **/
        /**********************************************************/
                                var modal_cancelamento = $("#modal-agendamentos-cancelamento-nao-permitido");
            
            $(document).on('click', '.btn_cancelar_agendamento', function(){
            limparErrosFormulario(modal_cancelamento);
            modal_cancelamento.modal('show');
            });

            function cancelamentoEstouCiente(){
            if($("#modal-cancelamento").find('[name=cancelamento_estou_ciente_flag]').is(':checked')){
                $("#modal-cancelamento").find('.confirmar').attr('disabled', false);
            }else{
                $("#modal-cancelamento").find('.confirmar').attr('disabled', true);
            }
            }

            $(document).ready(function(){
            cancelamentoEstouCiente();
            });

            $(document).on('click', '#modal-cancelamento [name=cancelamento_estou_ciente_flag]', function(){
            cancelamentoEstouCiente();
            });

            $(document).on('click', '#modal-cancelamento .confirmar', function(){
            var modal = $("#modal-cancelamento");
                limparErrosFormulario(modal);
            $.ajax({
                url: "https://atacadao.hodiebooking.com.br/agendamentos/2407050048370/cancelar",
                method: "PUT",
                data: modal.find('form').serialize(),
                    success: function(json){
                $(document).on('hide.bs.modal', '#modal-alert', function(){
                    window.location.href = json.protocolo;
                });
                alerta('Registro atualizado com sucesso');
                    },
                    statusCode:{
                        422: function(json){
                            var resp = json.responseJSON;
                            destacarErrosFormulario(resp.errors, modal);
                        }
                    }
            });
            });
