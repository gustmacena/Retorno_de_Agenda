  var selected = $("#modal-unidades").data('selected');
      selected = new Array();
  
  if($(this).prop('checked') == true){
    selected.push(obj);
  } else {
    $(selected).each(function(i, o){
      if(o.id_entidade == obj.id_entidade){
        selected.splice(i, 1);
      }
    });
  }
  $("#modal-unidades").data('selected', selected);
