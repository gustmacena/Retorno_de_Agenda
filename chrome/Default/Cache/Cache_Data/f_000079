<script>
function confirmar(msg, f_sim, f_nao){
  $('#modal-confirm .msg').text(msg);
  $('#modal-confirm .sim').unbind('click').click(function(){
		$('#modal-confirm').unbind('hidden.bs.modal').on('hidden.bs.modal', function(){
	    f_sim();
		});
    $('#modal-confirm').modal('hide');
  });
  $('#modal-confirm .nao').unbind('click').click(function(){
		$('#modal-confirm').unbind('hidden.bs.modal').on('hidden.bs.modal', function(){
	    if(typeof f_nao !== 'undefined'){
	      f_nao();
	    }
		});
    $('#modal-confirm').modal('hide');
  });
	$('#modal-confirm').unbind('hidden.bs.modal').on('hidden.bs.modal', function(){
		$('#modal-confirm .nao').click();
	});
  $('#modal-confirm').modal('show');
}
</script>
<script>
function alerta(msg, html){
  if(typeof html == 'undefined'){
    html = false;
  }
  
  if(html){
    $('#modal-alert .msg').html(msg);
  }else{
    $('#modal-alert .msg').text(msg);
  }
  
  $('#modal-alert').modal('show');
  $('#modal-alert .ok').click(function(){
    $('#modal-alert').modal('hide');
  });
}
</script>
        <script>
            $(document).on("click", "#modal-fornecedores .selecionar", function(){
                var selected = $("#modal-fornecedores").data('selected');
                $("#filtro-fornecedor .nome").val(selected[0].nome);
                $("#filtro-fornecedor .id").val(selected[0].id_entidade);
                $("#modal-fornecedores").modal('hide');
            });
        </script>
    <script>
$(document).on("click", "#modal-unidades .selecionar", function(){
  var selected = $("#modal-unidades").data('selected');

  
  $("#filtro-unidade .nome").val(selected[0].nome);
  $("#filtro-unidade .id").val(selected[0].id_entidade);
  $("#modal-unidades").modal('hide');
  var unidade_selecionada = selected[0].id_entidade


  var string = window.location.href,
    arr = string.split('/'),
    i;

for(i in arr){
    arr[i];
}

    rota = "https://atacadao.hodiebooking.com.br/agendamentos/calendario/:unidade";
    rota = rota.replace(':unidade', unidade_selecionada, rota);

    if(arr[i-1] =='calendario'){
        window.location.href = rota;
    }

  if(typeof $("#modal-unidades").data('function_adicional_after_save') != 'undefined'){
    $("#modal-unidades").data().function_adicional_after_save();
  }
});
</script>
    <script>
        $(this).ready(function(){
            $("#filtrosForm").find('[type=radio]:checked').attr('filtro_base_padrao', 'true');

            $("#filtrosForm").find('[type=hidden]').each(function() {
                $(this).attr('value_padrao', $(this).val());
            });

            $(document).on('submit', "#filtrosForm", function() {
                $("#filtrosForm #enviarFiltros").click();
                return false;
            });

            $(document).on('keypress', "#filtrosForm input[type=text]", function(event) {
                if (event.which == 13) {
                    $("#filtrosForm #enviarFiltros").click();
                    event.preventDefault();
                }
            });

            $(document).on('keypress', "#filtrosFormCheckin input[type=text]", function(event) {
                if (event.which == 13) {
                    $("#filtrosFormCheckin #enviarFiltrosCheckin").click();
                   event.preventDefault();
                }
            });


        });
    </script>


    <script>


        var vm;


        $(this).ready(function(){

            vm = new Vue({
                el: '.listagem-vue',
                methods: {
                    data(str){
                        var d = new Date(str);
                        return moment(d).format('DD/MM/YYYY');
                    },

                        link(obj){
        var url = 'https://atacadao.hodiebooking.com.br/cargas/:id';
        url = url.replace(':id', obj.id_carga);
        return url;
    },

        enviarForm() {
        window.document.getElementById('tipoSaida').value = "tela";
        VueTables.Event.$emit('vue-tables.filter::filtro', $("#filtrosForm").serialize());

        $("#filtros-collapse").collapse("toggle");
    },



    limparFiltros() {
        $("#filtrosForm").find('input,textarea').not('[type=checkbox],[type=radio]').val('');

        $("#filtrosForm").find('[type=checkbox],[type=radio]').prop('checked', false);

        $("#filtrosForm").find('[type=radio]').prop('checked', false);

        $("#filtrosForm").find('[type=radio]').each(function(){
            if($(this).attr('filtro_base_padrao') == 'true'){
                $(this).prop('checked', true);
            }
        });

        $("#filtrosForm").find('[type=hidden]').each(function() {
            $(this).val($(this).attr('value_padrao'));
        });

        $("#filtrosForm").find('option').prop('selected', false).change();

        VueTables.Event.$emit('vue-tables.filter::filtro', $("#filtrosForm").serialize());

        $("#filtros-collapse").collapse("toggle");
    },
                },

                data: {
                    senhaTexto:'',
                    options: {
                            headings: {"codigo":"N\u00b0 da carga","pallets":"Paletes","dthr_emissao":"Data de emiss\u00e3o","unidade":"Unidade","fornecedor":"Fornecedor","transportadora":"Transportadora","especie_carga":"Esp\u00e9cie de carga","acoes":""},
    sortable: [],
    customFilters: ['filtro'],
    initFilters: {
        filtro: $("#filtrosForm").serialize()
    }
                    },
                        columns: ["codigo","pallets","dthr_emissao","unidade","fornecedor","transportadora","especie_carga","acoes"],
                                    },
                created: function() {
                    $('.show_vue').show();
                }
            });

            datePicker(true);
            dateTimePicker(true);
            dateRangePicker();
            mascaraInteiro();
            masks();
        });

    </script>

    </body>
</html>
